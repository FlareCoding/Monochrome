<root type="widget">
    <DockPanel>
        <UserWidget path="mcxapp_custom_panel.mcx"/>
        <StackPanel backgroundColor="Color(20, 20, 20)" dockAnchor="right">
            <Button id="firstButton" text="Click Me" horizontalAlignment="center" marginTop="10"/>
            <Button text="Second Button" backgroundColor="Color(20, 120, 20)" horizontalAlignment="center" marginTop="20"/>
            <StackPanel orientation="horizontal" backgroundColor="Color(20, 20, 20)" horizontalAlignment="center" marginTop="20">
                <Label text="Sample checkbox" marginLeft="10" marginRight="20"/>
                <Checkbox text="status" marginRight="10"/>
            </StackPanel>
            <Combobox fixedWidth="140" fixedHeight="18" horizontalAlignment="center" marginTop="20" overlayBorderColor="Color(120, 200, 40)">
                <item value="Chicken"/>
                <item value="Beef"/>
                <item value="Tuna"/>
                <item value="Salmon"/>
                <item value="Protein Powder"/>
            </Combobox>
            <DropdownButton text="Dropdown" horizontalAlignment="center" marginTop="20">
                <MenuList>
                    <item value="Save"/>
                    <item value="Open"/>
                    <item value="Close"/>
                    <MenuList name="Zoom...">
                        <item value="200%"/>
                        <item value="150%"/>
                        <item value="100%"/>
                        <item value="50%"/>
                    </MenuList>
                </MenuList>
            </DropdownButton>
            <Entry horizontalAlignment="center" marginTop="20" placeholder="placeholder..."/>
            <ProgressBar id="mainProgressBar" horizontalAlignment="center" marginTop="20" value="30" fixedHeight="12"/>
            <TreeView horizontalAlignment="center" marginTop="20" minWidth="300" backgroundColor="Color(40, 40, 40)">
                <item name="Laptops" key="laptops">
                    <item name="Apple" key="apple">
                        <item name="Macbook Pro 13-inch"/>
                        <item name="Macbook Pro 15-inch"/>
                        <item name="Macbook Air"/>
                        <item name="Macbook Pro (2014 edition)"/>
                    </group>
                    <item name="Dell" key="dell"/>
                    <item name="HP"/>
                    <item name="Acer"/>
                </item>
                <item name="Phones">
                    <item name="iPhone 5"/>
                    <item name="iPhone 6"/>
                    <item name="iPhone 14"/>
                    <item name="Samsung Galaxy S7"/>
                    <item name="Nokia Flip Phone"/>
                </item>
            </TreeView>
        </StackPanel>
    </DockPanel>
</root>
